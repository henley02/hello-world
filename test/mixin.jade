mixin row(items)
    tr
        each item,index in items
            td= item

mixin table(tableData)
    table
        each row,index in tableData
            +row(row)

- var node=[{name:'express'},{name:'hapi'},{name:'derby'}]
+table(node)
- var js=[{name:"backbone"},{name:"angular"},{name:"ember"}]
+table(js)